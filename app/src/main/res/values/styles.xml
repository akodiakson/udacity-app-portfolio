<resources>

    <style name="MyTheme" parent="AppTheme">
        <item name="android:colorPrimary">@color/colorPrimary</item>
        <item name="android:colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="android:colorAccent">@color/colorAccent</item>
        <item name="android:windowActionBar">false</item>
    </style>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
    </style>

    <style name="ButtonStyle" parent="Base.Widget.AppCompat.Button">
        <item name="android:layout_width">240dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:layout_margin">4dp</item>
    </style>

    <style name="ButtonStyleFirstFive" parent="ButtonStyle">
        <item name="android:background">@color/colorPrimary</item>
    </style>

    <style name="ButtonStyleCapstone" parent="ButtonStyle">
        <item name="android:background">@color/colorAccent</item>
    </style>

    <style name="artist_search_result"></style>

    <style name="artist_search_result_thumb"></style>

    <style name="item_base_style">


    </style>

    <dimen name="search_result_margin_top">16dp</dimen>
    <dimen name="search_result_margin_bottom">16dp</dimen>
</resources>
